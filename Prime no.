c++
#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    int a,count = 0;
    cout<<"Enter Your No.";
    cin>>a;
    if(a== 1)
    {cout<<"Not Prime No.";
    return 0;
    }
    int i = 2;
    while(i <= sqrt(a))
    {
        if(a % i == 0)
       { cout<<"not prime";
        return 0;
       }
       i++;
    }
    cout<<"Prime No.";
}


in python
import math

a = input("Enter your number: ")
b = int(a)

if b <= 1:
    print("Not Prime")
else:
    for i in range(2, int(math.sqrt(b)) + 1):
        if b % i == 0:
            print("Not Prime")
            break
    else:
        print("Prime")

